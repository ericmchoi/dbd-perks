{
  "name": "dbd-perks",
  "description": "Simple webpage for looking up Dead by Daylight perk information",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --basepath dbd-perks",
    "deploy": "npx gh-pages -d __sapper__/export/dbd-perks",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "js-yaml": "^3.14.0",
    "marked": "^1.1.1",
    "polka": "next",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.9",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^7.0.1",
    "sapper": "^0.27.0",
    "svelte": "^3.24.1"
  }
}